<?xml version="1.0" encoding="UTF-8"?>
<form xsi:schemaLocation="iisis/schemas/2014/projects/singlec/iis/form ../../form/xsd/form.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns="iisis/schemas/2014/projects/singlec/iis/form"
      description="Профили подключения">

    <panel>
        <pageList>
            <page>
                <controlList>
                    <textBox ID="TextBox1"
                             description="TextBox1"
                             linkL="panel"
                             linkT="panel"
                             linkR="textBox2.linkL."
                             linkB="panel"
                             height="19"
                             clearButton="1"
                             openButton="1"
                             choiceButton="1"
                             useChoiceListHistory="1"
                             valueType="iis-sql:string_255"
                             picture="zoom">
                        <eventGroup>
                            <event name="$TextBoxOnChange"></event>
                        </eventGroup>
                    </textBox>

                    <textBox ID="textBox2"
                             description="textBox2"
                             linkL="panel"
                            linkB="panel"
                            linkR="panel"
                            linkT="panel">
                    </textBox>
                 </controlList>
            </page>
        </pageList>
    </panel>

</form>
