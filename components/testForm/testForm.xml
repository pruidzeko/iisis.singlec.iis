<?xml version="1.0" encoding="UTF-8"?>
<form xsi:schemaLocation="iisis/schemas/2014/projects/singlec/iis/form ../../../../xsd/2014/projects/singlec/iis/form/form.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns="iisis/schemas/2014/projects/singlec/iis/form"
      description="Форма Для Примера">

    <panel description="panel">
        <pageList>
            <page>
                <controlList>
                    <textBox ID="textBox1"
                             linkL="panel"
                             linkR="panel"
                             linkT="panel"
                             linkB="textBox2.linkT."
                             height="50%"/>
                    <textBox ID="textBox2"
                             top="10%"
                             linkL="panel"
                             linkR="panel"
                             linkT="textBox1.linkB."
                             linkB="panel"/>

                </controlList>
            </page>
        </pageList>
    </panel>
    <eventGroup>
        <event name="$FormBeforeClose">
            <event name="@FormBeforeClose"/>
        </event>
    </eventGroup>
</form>
